{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "title": "Root Schema",
    "type": "object",
    "default": {},
    "required": [
        "title",
        "salary",
        "equity",
        "companyHandle"
    ],
    "properties": {
        "title": {
            "title": "The title Schema",
            "type": "string",
            "default": "",
            "examples": [
                "new"
            ]
        },
        "salary": {
            "title": "The salary Schema",
            "type": "integer",
            "default": 0,
            "examples": [
                10
            ]
        },
        "equity": {
            "title": "The equity Schema",
            "type": "string",
            "default": "",
            "examples": [
                "0"
            ]
        },
        "companyHandle": {
            "title": "The companyHandle Schema",
            "type": "string",
            "default": "",
            "examples": [
                "c1"
            ]
        }
    },
    "examples": [{
        "id": 1,
        "title": "new",
        "salary": 10,
        "equity": "0",
        "companyHandle": "c1"
    }]
}